    "chicago"
  ],
  "author": "Dima Tretyak <itretyak@ya.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimatretyak/my-test-package/issues"
  },
  "homepage": "https://github.com/dimatretyak/my-test-package#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash.deburr": "^4.1.6",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
